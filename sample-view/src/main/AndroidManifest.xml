<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.allens.sample_view">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sample">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".act.CanvasAct"/>
        <activity android:name=".act.CanvasItemActivity"/>
        <activity android:name=".act.PaintAct"/>
        <activity android:name=".act.PaintItemActivity"/>
        <activity android:name=".act.MeasureAct"/>
        <activity android:name=".act.LayoutAct"/>
        <activity android:name=".act.PaintShaderItemActivity"/>
        <activity android:name=".act.PaintPathEffectItemActivity"/>
        <activity android:name=".act.CustomViewItemActivity"/>
        <activity android:name=".act.CustomViewAct"/>
        <activity android:name=".act.PaintXfermodeItemActivity"/>
    </application>

</manifest>